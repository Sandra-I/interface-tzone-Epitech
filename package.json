{
  "name": "T-Zone",
  "version": "1.0.0",
  "description": "TZone is a Chrome extension that allows you to select an area of the screen and retrieve the text present in this area directly in the clipboard.",
  "scripts": {
    "test": "npx jest --colors --coverage",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "webpack --watch --mode development",
    "lint": "npx eslint src/**/*.ts src/**/*.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/t-zone/interface.git"
  },
  "keywords": [
    "TZone",
    "OCR",
    "selection",
    "text",
    "image",
    "recognition"
  ],
  "author": "Sandra IBRAHIM, Florent MOUTON, Rodolphe TIQUET-DURIEZ, Youssef ZAKRAOUI, Reyane REMACHE",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/t-zone/interface/issues"
  },
  "homepage": "https://gitlab.com/t-zone/interface#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@hot-loader/react-dom": "^16.14.0",
    "@types/chrome": "0.0.125",
    "@types/dom-to-image": "^2.6.2",
    "@types/expect": "^24.3.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-wildcard": "^7.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "jest-canvas-mock": "^2.3.1",
    "jsdom-global": "^3.0.2",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "core-js": "^3.15.2",
    "dom-to-image": "^2.6.0",
    "eslint": "^8.12.0",
    "html2canvas": "^1.0.0-rc.7",
    "i18next": "^21.6.14",
    "jest": "^27.5.1",
    "jest-chrome": "^0.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-i18next": "^11.16.2",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.2.2",
    "react-select-event": "^5.4.0",
    "react-test-renderer": "^18.0.0",
    "rxjs": "^7.1.0",
    "sweetalert2": "^11.1.4"
  },
  "jest": {
    "testEnvironmentOptions": {
      "resources": "usable"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/localstorage-mock.ts",
      "<rootDir>/jest.setup.js"
    ]
  },
  "next-i18next": {
    "react": {
      "useSuspense": false
    }
  }
}
